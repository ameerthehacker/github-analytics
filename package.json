{
  "name": "gh-profile-tracker",
  "version": "0.0.1",
  "description": "Get insights into how many visit your profile and from which country",
  "scripts": {
    "dev": "concurrently -k npm:dev-client npm:dev-server",
    "dev-client": "react-scripts start",
    "build-client": "react-scripts build",
    "start-server": "ts-node --project src/api/tsconfig.json --files src/api/app.ts",
    "dev-server": "cross-env NODE_ENV=development nodemon --exec ts-node --project src/api/tsconfig.json --files src/api/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ameerthehacker/gh-profile-tracker.git"
  },
  "keywords": [
    "github",
    "profile",
    "tracker"
  ],
  "author": "ameerthehacker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ameerthehacker/gh-profile-tracker/issues"
  },
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.23",
    "express": "^4.17.1",
    "express-ip": "^1.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "react-router-dom": "^5.2.0",
    "emotion-theming": "^10.0.27"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
